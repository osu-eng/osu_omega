name = OSU Omega
description = Omega-based Drupal 7.x theme for the Ohio State University.
base theme = omega
screenshot = screenshot.png
engine = phptemplate
core = 7.x
stylesheets[all][] = css/osu_omega.normalize.css
stylesheets[all][] = css/osu_omega.hacks.css
stylesheets[all][] = css/osu_omega.styles.css

; Regions
regions[header] = Header
regions[navigation] = Navigation
; regions[highlighted] = Highlighted
; regions[help] = Help
regions[content] = Content
; regions[sidebar_first] = First Sidebar
; regions[sidebar_second] = Second Sidebar
regions[footer] = Footer

; Settings
settings[toggle_logo] = 0
settings[default_logo] = 0

; Panels layouts
plugins[panels][layouts] = layouts/panels
